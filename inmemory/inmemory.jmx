        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Obtain userIds">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def csvLines = props.get(&quot;csvData&quot;)

if (csvLines == null || csvLines.isEmpty()) {
    throw new IllegalStateException(&quot;CSV data is not loaded or empty&quot;)
}

def random = new Random()
def randomIndex = random.nextInt(csvLines.size())
def randomLine = csvLines.get(randomIndex)

def parts = randomLine.split(&quot;,&quot;)

vars.put(&quot;userId1&quot;, parts[0])
vars.put(&quot;userId2&quot;, parts[1])
vars.put(&quot;dialogId&quot;, parts[2])

// log.info(&quot;userId1: ${parts[0]}, userId2: ${parts[1]}, dialogId: ${parts[2]}&quot;)</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
