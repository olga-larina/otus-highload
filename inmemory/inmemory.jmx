        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Load userIds" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">if (props.get(&quot;csvData&quot;) == null) {
    String csvFilePath = &quot;/tmp/dialogs.csv&quot;;
    def csvLines = new File(csvFilePath).readLines();
    props.put(&quot;csvData&quot;, csvLines);
    log.info(&quot;CSV data loaded into Properties: ${csvLines.size()} lines.&quot;);
}</stringProp>
        </JSR223Sampler>
        <hashTree/>
      </hashTree>
